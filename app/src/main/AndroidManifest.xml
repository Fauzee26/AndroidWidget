<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fauzi.hilmy.androidwidget">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityDialogPrompt" />
        <activity android:name=".activityCustomAlertDialog" />
        <activity android:name=".ActivityListView" />
        <activity android:name=".ActivityGridView" />
        <activity android:name=".ActivitySpinner" />
        <activity android:name=".ActivityDatePicker" />
        <activity android:name=".ActivityTimePicker" />
        <activity android:name=".ActivityAnalogDigital"></activity>
    </application>

</manifest>